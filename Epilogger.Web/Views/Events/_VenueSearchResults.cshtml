@model IEnumerable<Epilogger.Web.Models.FoursquareVenue>

@foreach (var item in Model) {
    <div class="venuesearchresult-item">
    <strong>@item.Name</strong><br />
    @if (!string.IsNullOrEmpty(item.Address)) {
        @item.Address<br />
    }
    <a href="#" onclick="SetSelectedVenue('@item.id', '@item.Name', '@item.Address', '@item.City', '@item.State', '@item.Zip');">Select this venue</a>
    @*parent.$.fn.colorbox.close();*@
    </div>
}
