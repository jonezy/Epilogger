@model Epilogger.Web.Models.BrowseEventsDisplayViewModel

@{
    Layout = "~/Views/Shared/_InteriorLayout.cshtml";
    ViewBag.Title = "Epilogger - Browse (" + @Model.BrowsePageFilter + ")";
}

@switch (Model.BrowsePageFilter) {
    case "upcoming":
    case "past":
    case "now":
        @Html.Partial("_BrowseEventLists")
    break;
    default:
        @Html.Partial("_BrowseEventOverview")
    break;
}
